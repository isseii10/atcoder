{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: g++-10 build active file",
            "command": "g++",
            "args": [
                "-std=gnu++17",
                "-I",
                "/Users/isseiterada/atcoder/ac-library",
                "-Wall",
                "-Wextra",
                "-Wshadow",
                "-Wconversion",
                "-ggdb",
                "${file}",
                "-o",
                "${workspaceFolder}/a.out"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}